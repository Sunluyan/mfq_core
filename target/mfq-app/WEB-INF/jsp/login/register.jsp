<script>
var sendCodeURL = "/api/customer/sendRegiterCode";
var hasSendCode = false;
var totalTime = 120;
var lastTime = totalTime;

$(function(){
	$('#send_code').bind('click',function(){
		sendMobile();
	});
});


function sendMobile(){
	if(hasSendCode){
		return false;
	}
	var phone = $('#input_phone').val();
	if(!phone){
		alert('请填写手机号');
		return false;
	}
	
	var  url = sendCodeURL + "&phone=" + phone;
	$.getJSON(url,function(json){
		if(json['code'] == 22000){
			hasSendCodeShow();
		} else {
			alert("发送失败:" + json['message']);
		}
	});
}

function hasSendCodeShow(){
	hasSendCode = true;
	$("#send_code").removeClass('btns-success').addClass('btns-default');
	timeCount();
}


function timeCount(){
	$("#send_code").text(lastTime);

	if(lastTime <= 0){
		$("#send_code").removeClass('btns-default').addClass('btns-success').text('获取验证码');
		hasSendCode = false;
        lastTime = totalTime;
        return true;
	}

	lastTime--;
	setTimeout(function() {
		timeCount();
	}, 1000);
}
</script>


    